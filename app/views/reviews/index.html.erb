<div class="container">
  <% @lists = List.all %>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">List Name</th>
        <th scope="col">Comment</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <% @lists.each do |list| %>
    <tbody>
      <td><%= list.name %></td>
      <td><%= link_to 'Add Review', new_list_review_path %></td>
    </tbody>
    <% end %>
  </table>
</div>
